{
  "name": "mininote",
  "version": "2.0.0",
  "description": "",
  "scripts": {
    "prebuild": "pnpm install",
    "init:server": "cd server && pnpm i",
    "init:client": "cd client && pnpm i",
    "init:both": "concurrently \"pnpm init:server\" \"pnpm init:client\"",
    "dev:server": "cd server && pnpm dev",
    "dev:client": "cd client && pnpm dev",
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "build:client": "cd client && pnpm install --no-frozen-lockfile && pnpm build",
    "build:server": "cd server && pnpm install --production --frozen-lockfile",
    "build": "pnpm build:client && pnpm build:server",
    "start": "cd server && pnpm start",
    "preview:client": "cd client && pnpm build && pnpm start --port 3000",
    "preview:server": "cd server && pnpm start",
    "preview": "pnpm init:both && concurrently \"pnpm preview:server\" \"pnpm preview:client\""
  },
  "keywords": [],
  "author": "Saicharan kaparthi",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
